{application, interclock, [
  {description, "Storage for an ITC-based database"},
  {vsn, "0.1.1"},
  {registered, [interclock_sup, interclock_manager]},
  {applications, [
    kernel,
    stdlib,
    itc,
    bitcask,
    gproc,
    uuid
  ]},
  {mod, {interclock_app, []}},
  {env, [
    {gc_delay, 4320000}, % 50 days (in secs)
    {gc_interval, 604800} % 7 days (in secs)
  ]}
]}.
